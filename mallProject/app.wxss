/**app.wxss**/
@import "./style/iconfont.wxss";
page,view,text,swiper,swiper-item,image,navigator{
  padding:0;
  margin:0;
  box-sizing:border-box;
}
/*
  主题颜色 通过变量来实现
  1 less中存在变量这个知识
  2 原生的css和wxss也支持变量
*/
page{
  --themeColor:#eb4450;
  /*定义统一的字体大小 假设设计稿大小是375px*/
  font-size:28rpx;
}
image{
  width:100%;
}