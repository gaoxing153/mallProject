/**index.wxss**/
swiper{
  width:100%;
  height:calc(750rpx*340/750)
}
swiper-item > image{
  width:100%;
  /* height:calc(750rpx*340/750) */
}
.catitems{
  display:flex;
  align-items: center;
  flex-direction:row;
  justify-content:center;
}
.catitems > navigator{
  padding: 20rpx;
  display: flex;
  flex: 1;
  /* width: 25%; */
}
.catitems > navigator > image{
  width:100%;
}
.index_floor > .floor_group > .floor_tile > image{
  width:100%
}
/* .index_floor > .floor_group > .floor_list{
  overflow: hidden;
} */
.index_floor > .floor_group > .floor_list > navigator{
  float:left;
  width:33.33%;
}
.index_floor > .floor_group > .floor_list > navigator:nth-last-child(-n+4){
  height:27.27vw;
  padding: 5rpx;
}
.index_floor > .floor_group > .floor_list > navigator > image{
  height:100%;
  width:100%;
}
.userinfo {
  display: flex;
  flex-direction: column;
  align-items: center;
}

.userinfo-avatar {
  width: 128rpx;
  height: 128rpx;
  margin: 20rpx;
  border-radius: 50%;
}

.userinfo-nickname {
  color: #aaa;
}

.usermotto {
  margin-top: 200px;
}